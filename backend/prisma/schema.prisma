generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id          Int      @id @default(autoincrement())
  name        String
  rollNumber  String   @unique
  department  String
  status      String   @default("Pending")
  createdAt   DateTime @default(now())
}
